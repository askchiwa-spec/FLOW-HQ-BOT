# Database
DATABASE_URL=postgresql://user:password@localhost:5432/flowhq

# Admin Auth
ADMIN_PASSWORD=your-secure-password

# Portal API Security (shared between control-plane and web app)
PORTAL_INTERNAL_KEY=change-this-to-a-long-random-secret-key

# App
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Paths
SESSIONS_PATH=./sessions
LOGS_PATH=./logs

# Worker Settings
STALE_THRESHOLD_MINUTES=2
STALE_CHECK_INTERVAL_MS=60000
